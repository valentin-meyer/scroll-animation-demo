{
  "name": "@wingsuit-designsystem/wingsuit",
  "version": "0.5.3",
  "description": "A design system with Storybook, Tailwind and Drupal",
  "main": "",
  "license": "GPL-2.0",
  "author": {
    "name": "KEY-TEC",
    "url": "https://github.com/wingsuit-designsystem/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wingsuit-designsystem/wingsuit.git"
  },
  "keywords": [
    "storybook",
    "prototyping"
  ],
  "scripts": {
    "dev:storybook": "start-storybook --config-dir apps/storybook",
    "build:storybook": "build-storybook --config-dir apps/storybook -o ./dist/app-storybook",
    "build-storybook": "build-storybook",
    "dev:drupal": "cross-env-shell NODE_ENV=development \"webpack --watch --config ./apps/drupal/webpack.config.js\"",
    "build:drupal": "cross-env-shell NODE_ENV=production \"webpack --config ./apps/drupal/webpack.config.js\"",
    "lint:js": "eslint --ext .js ./",
    "lint:css": "stylelint '**/*.css'",
    "lint": "npm run lint:js; npm run lint:css;",
    "fmt:js": "eslint --fix --ext .js,.vue ./",
    "fmt:css": "npm run lint:css --fix && prettier --loglevel warn --write '**/*.{css}'",
    "fmt": "npm run fmt:js; npm run fmt:css",
    "test:backstop:ref": "node tools/tests/vrt/backstop-example-reference.js",
    "test:backstop:test": "node tools/tests/vrt/backstop-example-test.js",
    "test:pa11y": "pa11y-ci --config tools/tests/accessibility/pa11y.js",
    "test:unit:default": "NODE_ENV=test jest --projects source/default/",
    "test:unit": "npm run test:unit:default",
    "test": "npm run test:unit",
    "ci": "npm run lint && npm run test && npm run build:drupal"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@storybook/addon-viewport": "^6.0.0-beta.6",
    "@storybook/linter-config": "^2.5.0",
    "@storybook/react": "^6.0.0-beta.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@wingsuit-designsystem/cli": "^0.5.3",
    "@wingsuit-designsystem/core": "^0.5.3",
    "@wingsuit-designsystem/pattern": "^0.5.3",
    "@wingsuit-designsystem/storybook": "^0.5.3",
    "autoprefixer": "^9.7.4",
    "core-js": "^3.4.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.0.1",
    "fibers": "^4.0.2",
    "holderjs": "^2.9.6",
    "lodash": "^4.17.15",
    "postcss-nested": "^4.2.1",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.3",
    "require-context": "^1.1.0",
    "stylelint-scss": "^3.17.2",
    "webpack": "^4.41.3",
    "yo": "^3.1.1"
  },
  "dependencies": {
    "alpinejs": "^1.10.1",
    "fullPage": "https://github.com/alvarotrigo/fullPage.js.git",
    "lax.js": "https://github.com/alexfoxy/lax.js.git",
    "svg4everybody": "^2.1.9",
    "tailwindcss": "^1.4.0"
  },
  "gitHead": "f33e3b40f1bad944ca7cd0c1aec27618cc56f9ef"
}
