{#
/**
 * @file
 * image--primary
 *   Implements images src and alt.
 *
 * Available variables for image:
 *  - Inherit variables from @atoms/_image.twig
 */
#}
{% if image is empty and src is not empty %}
  <img {{ img_attributes }} src="{{ src }}" alt="{{ alt }}" />
{% else %}
  {{ image }}
{% endif %}
